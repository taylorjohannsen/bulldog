<div class="cats">
    <div class="col-md-6 m-auto">
      <div class="card card-body text-center">
        <h1><i class="fab fa-node-js fa-3x"></i></h1>
        <p>Create an account or login</p>
        <a href="/admin/register" class="btn btn-primary btn-block mb-2">Register</a>
        <a href="/admin/login" class="btn btn-secondary btn-block">Login</a>
      </div>
    </div>
  </div>


  <% listings.forEach(function(listing) { %>
    <a href='/admin/inventory/<%= listing._id %>'>
        <p class="lead mb-3">Title, <%= listing.title %></p>
    </a>
    <p class="lead mb-3">desc, <%= listing.desc %></p>
    <p class="lead mb-3">warehouse id, <%= listing.whid %></p>
    <p class="lead mb-3">price, <%= listing.price %></p>
    <p class="lead mb-3">date, <%= listing.date %></p>
    <% if (listing.photos.length > 0) { %>
        <img class='test' src='<%= listing.photos[0].path %>'>
    <% }; %>

<% }); %>
